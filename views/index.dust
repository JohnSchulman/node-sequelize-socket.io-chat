<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">
    <title>{title}</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.blue-indigo.min.css" />
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
<!--    <link rel='stylesheet' href='/stylesheets/style.css'/>-->
    <script src="/javascripts/socket.io-client/dist/socket.io.js"></script>
    <script src="/javascripts/dest/Observable.js"></script>
    <script src="/javascripts/dest/Authors.js"></script>
    <script src="/javascripts/dest/Socket.js"></script>
    <script src="/javascripts/dest/Script.js"></script>
    <style>
        .demo-layout-waterfall .mdl-layout__header-row .mdl-navigation__link:last-of-type  {
            padding-right: 0;
        }
        .mdl-navigation__link.is-active {
            background-color:#e0e0e0;
            color:#000
        }
        .mdl-layout__header-row .mdl-navigation__link.is-active, .mdl-layout__header-row .mdl-navigation__link:hover {
            background: rgba(224,224,224, 0.5);
        }
        .mdl-layout__header-row .mdl-navigation__link:last-child {
            padding-right: 24px!important;
        }

        .mdl-accordion {
            cursor: pointer;
        }
        .mdl-accordion.mdl-accordion--opened {
            border-top: 1px solid #e0e0e0;
            border-bottom: 1px solid #e0e0e0;
            margin-top: -1px;
        }
        .mdl-accordion.mdl-accordion--opened + .mdl-accordion.mdl-accordion--opened {
            border-top: none;
            margin-top: 0;
        }
        .mdl-accordion .mdl-accordion__content-wrapper {
            overflow: hidden;
        }
        .mdl-accordion .mdl-accordion__content {
            transition-property: margin-top;
            transition-duration: 0.2s;
        }
        .mdl-accordion .add-new-discussion {
            color: rgba(0, 0, 0, 0.3);
            position: absolute;
            right: 40px;
            margin-top: -10px;
        }
        .mdl-accordion .mdl-accordion__icon {
            transition-property: transform;
            transition-duration: 0.2s;
            color: rgba(0, 0, 0, 0.3);
            position: absolute;
            right: 6px;
            margin-top: -3px;
        }
        .mdl-accordion.mdl-accordion--opened .mdl-accordion__icon {
            transform: rotate(-180deg);
        }
        .mdl-accordion.mdl-accordion--opened .mdl-accordion__button {
            background-color:#e0e0e0;
            color:#000;
        }
        .mdl-accordion.mdl-accordion--opened .mdl-accordion__content {
            margin-top: 0 !important;
        }
    </style>
</head>
<body>

<div class="demo-layout-waterfall mdl-layout mdl-js-layout">
    <header class="mdl-layout__header mdl-layout__header--waterfall">
        <!-- Top row, always visible -->
        <div class="mdl-layout__header-row">
            <!-- Title -->
            <span class="mdl-layout-title">{title}</span>
            <div class="mdl-layout-spacer"></div>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--expandable
                  mdl-textfield--floating-label mdl-textfield--align-right">
                <label class="mdl-button mdl-js-button mdl-button--icon"
                       for="waterfall-exp">
                    <i class="material-icons">search</i>
                </label>
                <div class="mdl-textfield__expandable-holder">
                    <input class="mdl-textfield__input" type="text" name="sample" id="waterfall-exp" placeholder="Recherche">
                </div>
            </div>
        </div>
        <!-- Bottom row, not visible on scroll -->
        <div class="mdl-layout__header-row">
            <div class="mdl-layout-spacer"></div>
            <!-- Navigation -->
            <nav class="mdl-navigation discussions"></nav>
        </div>
    </header>
    <div class="mdl-layout__drawer">
        <span class="mdl-layout-title">{title}</span>
        <nav class="mdl-navigation">
            <div class="mdl-accordion mdl-accordion--opened">
                <a class="mdl-navigation__link mdl-accordion__button">
                    <i class="material-icons mdl-accordion__icon mdl-animation--default">expand_more</i>
                    Discussions
                    <button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-accordion__icon-add add-new-discussion">
                        <i class="material-icons">add</i>
                    </button>
                </a>
                <div class="mdl-accordion__content-wrapper">
                    <div class="mdl-accordion__content mdl-animation--default discussions"></div>
                </div>
            </div>
            <div class="mdl-accordion">
                <a class="mdl-navigation__link mdl-accordion__button">
                    <i class="material-icons mdl-accordion__icon mdl-animation--default">expand_more</i>
                    Connectés
                </a>
                <div class="mdl-accordion__content-wrapper">
                    <div class="mdl-accordion__content mdl-animation--default">
                        <a class="mdl-navigation__link">Here</a>
                        <a class="mdl-navigation__link">There</a>
                    </div>
                </div>
            </div>
        </nav>
    </div>
    <main class="mdl-layout__content">
        <div class="page-content">
            <h3 class="discussion-title"></h3>
            <ul class="messages"></ul>
            <p class="is_writing"></p>
            <textarea class="message" placeholder="Votre message ici..."></textarea>
            <br />
            <p class="message-info"></p>
            <br />
            <button type="button" class="send">Envoyer</button>
            <button type="button" class="disconnect">Sortir de la discussion</button>
        </div>
    </main>
</div>

